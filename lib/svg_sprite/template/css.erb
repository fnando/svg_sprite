/* Generated by sprite_svg v<%= SvgSprite::VERSION %> at <%= Time.now %> */
<% for svg in source %>
.<%= options[:name] %>-<%= svg.name %>,
.<%= options[:name] %>-<%= svg.name %>-before:before,
.<%= options[:name] %>-<%= svg.name %>-after:after {
  background-image: url(<%= svg.data_uri %>);
  <%- if svg.dimensions? -%>
  width: <%= svg.width %>;
  height: <%= svg.height %>;
  <%- end -%>
}
<% end %>
